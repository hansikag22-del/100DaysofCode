//Q77: Check if the elements on the diagonal of a matrix are distinct.

/*
Sample Test Cases:
Input 1:
3 3
1 2 3
4 5 6
7 8 1
Output 1:
False

Input 2:
3 3
1 2 3
4 5 6
7 8 9
Output 2:
True

*/

#include <stdio.h>

int main() {
    int n, m;
    
    // Read matrix size
    printf("Enter number of rows and columns: ");
    scanf("%d %d", &n, &m);
    
    int matrix[n][m];
    
    // Read matrix elements
    printf("Enter the elements of the matrix:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    
    // Diagonal check is valid only for square matrices
    if (n != m) {
        printf("Matrix must be square.\n");
        return 0;
    }
    
    int distinct = 1;
    
    // Check for distinct diagonal elements
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (matrix[i][i] == matrix[j][j]) {
                distinct = 0;
                break;
            }
        }
        if (!distinct)
            break;
    }
    
    // Print result
    if (distinct)
        printf("True\n");
    else
        printf("False\n");
    
    return 0;
}

