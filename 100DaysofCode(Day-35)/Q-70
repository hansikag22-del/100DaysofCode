//Q70: Rotate an array to the right by k positions.

/*
Sample Test Cases:
Input 1:
5
1 2 3 4 5
2
Output 1:
4 5 1 2 3

*/


#include <stdio.h>

int main() {
    int n, k;
    
    // Read number of elements
    printf("Enter number of elements: ");
    scanf("%d", &n);
    
    int arr[n];
    
    // Read array elements
    printf("Enter %d elements:\n", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    
    // Read rotation count
    printf("Enter number of positions to rotate: ");
    scanf("%d", &k);
    
    // Adjust k if greater than n
    k = k % n;
    
    int result[n];
    int index = 0;
    
    // Copy last k elements to the beginning
    for (int i = n - k; i < n; i++) {
        result[index++] = arr[i];
    }
    
    // Copy remaining elements
    for (int i = 0; i < n - k; i++) {
        result[index++] = arr[i];
    }
    
    // Print rotated array
    printf("Rotated array:\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", result[i]);
    }
    printf("\n");
    
    return 0;
}


