//Q65: Search in a sorted array using binary search.

/*
Sample Test Cases:
Input 1:
5
1 3 5 7 9
7
Output 1:
Found at index 3

Input 2:
5
1 3 5 7 9
6
Output 2:
-1

*/

#include <stdio.h>

int main() {
    int n, i, key, low, high, mid;
    int arr[100];

    scanf("%d", &n);             // number of elements
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);    // input sorted array
    }

    scanf("%d", &key);           // element to search
    low = 0;
    high = n - 1;

    while (low <= high) {
        mid = (low + high) / 2;  // find middle
        if (arr[mid] == key) {
            printf("Found at index %d\n", mid);
            return 0;            // element found
        } else if (arr[mid] < key)
            low = mid + 1;       // search right half
        else
            high = mid - 1;      // search left half
    }

    printf("-1\n");              // not found
    return 0;
}

